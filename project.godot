window/size/width=1280
window/size/height=720

[editor]

import/use_multiple_threads=false
search_in_file_extensions=PackedStringArray("gd", "gdshader", "shader", "conf", "db")
convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PackedStringArray("res://addons/game_data_manager/plugin.cfg", "res://addons/tiled_importer/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[gui]

theme/default_font_subpixel_positioning=0
