; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Source of Mana"
config/version="0.8"
run/main_scene="res://presets/Default.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="SourceOfMana"
config/features=PackedStringArray("4.4")
run/flush_stdout_on_print=true
run/max_fps=60
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://data/press/splash/splashscreen.png"
config/icon="res://data/press/logo/logo_seed.svg"

[audio]

buses/default_bus_layout="res://presets/audio/default_bus_layout.tres"

[autoload]

Launcher="*res://sources/launcher/Launcher.gd"
Network="*res://sources/network/Network.gd"
FSM="*res://sources/launcher/FSM.gd"

[debug]

gdscript/warnings/unused_signal=0
gdscript/warnings/treat_warnings_as_errors=true
gdscript/completion/autocomplete_setters_and_getters=true

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/aspect="ignore"
window/vsync/vsync_mode=0
mouse_cursor/tooltip_position_offset=Vector2(16, 16)
window/size/width=1280
window/size/height=720

[editor]

import/use_multiple_threads=false
search_in_file_extensions=PackedStringArray("gd", "gdshader", "shader", "conf", "db")
convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-sqlite/plugin.cfg", "res://addons/tiled_importer/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[gui]

theme/default_font_subpixel_positioning=0
theme/default_font_antialiased=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

