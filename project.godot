[input_devices]

pointing/android/enable_pan_and_scale_gestures=true
pointing/emulate_touch_from_mouse=true

[physics]

2d/run_on_separate_thread=true
common/max_physics_steps_per_frame=1
2d/physics_engine="GodotPhysics2D"
common/physics_interpolation=true

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
textures/lossless_compression/force_png=true
shading/overrides/force_vertex_shading=true
shading/overrides/force_lambert_over_burley=true
camera/depth_of_field/depth_of_field_bokeh_shape=0
anti_aliasing/screen_space_roughness_limiter/enabled=false
textures/decals/filter=0
textures/light_projectors/filter=0
global_illumination/sdfgi/frames_to_converge=0
global_illumination/sdfgi/frames_to_update_lights=4
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
2d/snap/snap_2d_transforms_to_pixel=true
environment/default_environment="res://default_env.tres"
threads/thread_safe_bvh=true
2d/snapping/use_gpu_pixel_snap=false
quality/driver/fallback_to_gles2=true
vram_compression/import_etc=true
vram_compression/import_pvrtc=true

[tiled_importer]

enable_json_format=false

[xr]

shaders/enabled=true
